{
  "extends": "../../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "wb"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "wb"
      ],
      "kebab-case"
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-snake-case",
        "allow-leading-underscore"
      ]
    }
  }
}

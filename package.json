{
  "name": "scion-workbench",
  "version": "0.0.0-beta.35",
  "description": "SCION Workbench provides a workbench layout for Angular applications useful for applications with non-linear workflows where users want to flexibly view and edit content in parallel.",
  "license": "EPL-2.0",
  "homepage": "https://github.com/SchweizerischeBundesbahnen/scion-workbench",
  "bugs": {
    "url": "https://github.com/SchweizerischeBundesbahnen/scion-workbench/issues"
  },
  "author": {
    "name": "SCION contributors"
  },
  "scripts": {
    "before-push": "run-s build lint test:headless e2e:headless",
    "build": "run-s **:*:build",
    "lint": "run-p **:*:lint",
    "test": "run-s **:*:test",
    "test:headless": "cross-env HEADLESS=true run-s **:*:test",
    "e2e": "run-s e2e:*",
    "e2e:headless": "run-s e2e:*:headless",
    "e2e:application-platform": "run-s \"application-platform:e2e:serve -- {@}\" --",
    "e2e:application-platform:headless": "cross-env HEADLESS=HEADLESS run-s \"application-platform:e2e:serve -- {@}\" --",
    "e2e:workbench": "run-s \"workbench:e2e -- {@}\" --",
    "e2e:workbench:headless": "cross-env HEADLESS=HEADLESS run-s \"workbench:e2e -- {@}\" --",
    "postinstall": "ngcc --tsconfig \"tsconfig.base.json\"",

    "dimension:lint": "ng lint @scion/dimension",
    "dimension:test": "ng test @scion/dimension",
    "dimension:build": "ng build --prod @scion/dimension",

    "viewport:lint": "ng lint @scion/viewport",
    "viewport:test": "ng test @scion/viewport",
    "viewport:build": "ng build --prod @scion/viewport",
    "postviewport:build": "scss-bundle -p . -e projects/scion/viewport/src/theme/_scrollbar.scss -o dist/scion/viewport/scrollbar.scss",

    "mouse-dispatcher:lint": "ng lint @scion/mouse-dispatcher",
    "mouse-dispatcher:build": "ng build --prod @scion/mouse-dispatcher",

    "workbench:lint": "ng lint @scion/workbench",
    "workbench.e2e:lint": "ng lint @scion/workbench.e2e",
    "workbench:test": "ng test @scion/workbench",
    "workbench:e2e": "ng e2e @scion/workbench.e2e",
    "workbench:build": "ng build --prod @scion/workbench",
    "preworkbench:e2e": "npx webdriver-manager update --versions.chrome=84.0.4147.0",
    "postworkbench:build": "scss-bundle -p . -e projects/scion/workbench/src/theme/_theming.scss -o dist/scion/workbench/theming.scss",

    "application-platform.api:lint": "ng lint @scion/workbench-application-platform.api",
    "application-platform.api:build": "ng build --prod @scion/workbench-application-platform.api",

    "application-platform:lint": "ng lint @scion/workbench-application-platform",
    "application-platform.e2e:lint": "ng lint @scion/workbench-application-platform.e2e",
    "application-platform:test": "ng test @scion/workbench-application-platform",
    "application-platform:build": "ng build --prod @scion/workbench-application-platform",
    "application-platform:e2e:ngserve": "run-p -r application-platform-apps:ngserve \"application-platform:await-then-protractor -- {@}\" --",
    "application-platform:e2e:serve": "run-p -r application-platform-apps:serve \"application-platform:await-then-protractor -- {@}\" --",
    "application-platform:await-then-protractor": "run-s application-platform-apps:*:await \"application-platform:run-protractor -- {@}\" --",
    "application-platform:run-protractor": "ng e2e @scion/workbench-application-platform.e2e",
    "preapplication-platform:run-protractor": "npx webdriver-manager update --versions.chrome=84.0.4147.0",

    "application-platform.application-core:lint": "ng lint @scion/workbench-application.core",
    "application-platform.application-core:test": "ng test @scion/workbench-application.core",
    "application-platform.application-core:build": "ng build --prod @scion/workbench-application.core",

    "application-platform.application-angular:lint": "ng lint @scion/workbench-application.angular",
    "application-platform.application-angular:test": "ng test @scion/workbench-application.angular",
    "application-platform.application-angular:build": "ng build --prod @scion/workbench-application.angular",

    "app-common:lint": "ng lint app-common",
    "app-common:build": "ng build --prod app-common",

    "workbench-testing-app:serve": "ng serve workbench-testing-app",
    "workbench-testing-app:lint": "ng lint workbench-testing-app",
    "workbench-testing-app-localhost:build": "ng build workbench-testing-app --configuration=production-localhost",

    "application-platform-apps:ngserve": "run-p application-platform-apps:*:ngserve",
    "application-platform-apps:serve": "run-p application-platform-apps:*:serve",

    "application-platform-apps:host-app:ngserve": "ng serve application-platform-host-app --port 5000 --prod",
    "application-platform-apps:host-app:serve": "cd dist/workbench-application-platform-host-app-localhost && http-server --cors --silent --port 5000",
    "application-platform-apps:host-app:lint": "ng lint application-platform-host-app",
    "application-platform-apps:host-app:await": "wait-on http-get://localhost:5000",
    "application-platform-apps:host-app-vercel:build": "ng build application-platform-host-app --configuration=production-vercel",
    "application-platform-apps:host-app-localhost:build": "ng build application-platform-host-app --configuration=production-localhost",

    "application-platform-apps:contact-app:ngserve": "ng serve application-platform-contact-app --port 5001",
    "application-platform-apps:contact-app:serve": "cd dist/workbench-application-platform-contact-app-localhost && http-server --cors --silent --port 5001",
    "application-platform-apps:contact-app:lint": "ng lint application-platform-contact-app",
    "application-platform-apps:contact-app:await": "wait-on http-get://localhost:5001",
    "application-platform-apps:contact-app-vercel:build": "ng build application-platform-contact-app --configuration=production-vercel",
    "application-platform-apps:contact-app-localhost:build": "ng build application-platform-contact-app --configuration=production-localhost",

    "application-platform-apps:communication-app:ngserve": "ng serve application-platform-communication-app --port 5002",
    "application-platform-apps:communication-app:serve": "cd dist/workbench-application-platform-communication-app-localhost && http-server --cors --silent --port 5002",
    "application-platform-apps:communication-app:lint": "ng lint application-platform-communication-app",
    "application-platform-apps:communication-app:await": "wait-on http-get://localhost:5002",
    "application-platform-apps:communication-app-vercel:build": "ng build application-platform-communication-app --configuration=production-vercel",
    "application-platform-apps:communication-app-localhost:build": "ng build application-platform-communication-app --configuration=production-localhost",

    "application-platform-apps:devtools-app:ngserve": "ng serve application-platform-devtools-app --port 5003",
    "application-platform-apps:devtools-app:serve": "cd dist/workbench-application-platform-devtools-app-localhost && http-server --cors --silent --port 5003",
    "application-platform-apps:devtools-app:lint": "ng lint application-platform-devtools-app",
    "application-platform-apps:devtools-app:await": "wait-on http-get://localhost:5003",
    "application-platform-apps:devtools-app-vercel:build": "ng build application-platform-devtools-app --configuration=production-vercel",
    "application-platform-apps:devtools-app-localhost:build": "ng build application-platform-devtools-app --configuration=production-localhost",

    "application-platform-apps:testing-app:ngserve": "ng serve application-platform-testing-app --port 5004",
    "application-platform-apps:testing-app:serve": "cd dist/workbench-application-platform-testing-app-localhost && http-server --cors --silent --port 5004",
    "application-platform-apps:testing-app:lint": "ng lint application-platform-testing-app",
    "application-platform-apps:testing-app:await": "wait-on http-get://localhost:5004",
    "application-platform-apps:testing-app-vercel:build": "ng build application-platform-testing-app --configuration=production-vercel",
    "application-platform-apps:testing-app-localhost:build": "ng build application-platform-testing-app --configuration=production-localhost",

    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && markdown-include docs/site/changelog/changelog.template.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.4",
    "@angular/cdk": "10.0.2",
    "@angular/common": "10.0.4",
    "@angular/compiler": "10.0.4",
    "@angular/core": "10.0.4",
    "@angular/forms": "10.0.4",
    "@angular/platform-browser": "10.0.4",
    "@angular/platform-browser-dynamic": "10.0.4",
    "@angular/router": "10.0.4",
    "@scion/toolkit": "10.0.0-beta.3",
    "@scion/toolkit.internal": "10.0.0-beta.3",
    "js-sha256": "0.9.0",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.3",
    "@angular-devkit/build-ng-packagr": "0.1000.3",
    "@angular/cli": "10.0.3",
    "@angular/compiler-cli": "10.0.4",
    "@angular/language-service": "10.0.4",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.50",
    "codelyzer": "6.0.0",
    "conventional-changelog-cli": "2.0.34",
    "cross-env": "7.0.2",
    "http-server": "0.12.3",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "markdown-include": "0.4.3",
    "ng-packagr": "10.0.1",
    "npm-run-all": "4.1.5",
    "protractor": "7.0.0",
    "puppeteer": "5.1.0",
    "rxjs-tslint-rules": "4.34.0",
    "scss-bundle": "3.1.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.9.7",
    "vercel": "19.1.2",
    "wait-on": "5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SchweizerischeBundesbahnen/scion-workbench.git"
  }
}
